1.Basic Arithmetic Operations 8:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b;
    scanf("%d",&a);
    scanf("%d",&b);
    printf("Sum: %d",a+b);
    printf("\nDifference: %d",a-b);
    printf("\nProduct: %d",a*b);
    printf("\nQuotient: %d",a/b);
    printf("\nRemainder: %d",a%b); 
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

2.Smart Shopper 1:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int a,a1,b,b1,c,c1,d;
     float tot1,tot2;
     scanf("%d %d",&a,&a1);
     scanf("%d %d",&b,&b1);
     scanf("%d %d",&c,&c1);
     scanf("%d",&d);
     tot1=(a*a1)+(b*b1)+(c*c1);
     printf("Total (Before Tax): %.2f",tot1);
     tot2=tot1+(tot1*10/100);
     printf("\nTotal (After Tax): %.2f",tot2);
     if(tot2<=d){
           printf("\nWithin Budget");}

    else{
           printf("\nOver Budget");
}
    return 0;
}

3.MINIMUM CUTS TO DIVIDE A CIRCLE 2:

int numberOfCuts(int n) {
    if(n==1)
     return 0;
    if(n%2==0)
      return n/2;
    return n;
 }   
   
4.DIVISOR GAME 2:

bool divisorGame(int n) {
    if(n%2==0)
       return true;
    else 
       return false;
}

5.Greatest of Three Numbers 11:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int a,b,c;
     scanf("%d",&a);
     scanf("%d",&b);
     scanf("%d",&c);
     if((a>b)&&(a>c))
{
printf("%d is the greatest",a);
}
else if((b>c)&&(b>a)){
printf("%d is the greatest",b);
}
else if((a==b)||(b==c)){
printf("There is a tie for the greatest number");
}
else
 printf("%d is the greatest",c);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

6.Character Classifier 1:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char c;
    scanf("%c",&c);
    if(c>='A'&&c<='Z'){
printf("Uppercase Alphabet");
}
else if(c>='a'&&c<='z'){
printf("Lowercase Alphabet");
}
else if(c>='0'&&c<='9'){
printf("Digit");
}
else{
printf("Special Character");
}
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

7.Triangle Type Detector:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int a, b, c;
    scanf("%d %d %d", &a, &b, &c);

    if (a + b <= c || b + c <= a || c + a <= b) {
        printf("Not a valid triangle");
    }
    else if (a == b && b == c) {
        printf("Equilateral Triangle\n");
    }
    else if (a == b || b == c || a == c) {
        printf("Isosceles Triangle\n");
    }
    else {
        printf("Scalene Triangle\n");
    }

    return 0;
}

8.Leap Year Checker 28:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int year;
     scanf("%d",&year);
     if((year%400==0)||(year%4==0)&&(year%100!=0)){
printf("Leap Year");
       }
else {
  printf("Not a Leap Year");
}
    return 0;
}

9.Print All Factors:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d",&N);
    for(int i=1;i<=N/2;i++){
      if(N%i==0){
        printf("%d ",i);
}
}
 printf("%d ",N);  
    return 0;
}

10.Is It a Prime Number? :

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n, i, a = 0;
  scanf("%d", &n);
  if (n == 0 || n == 1)
    a = 1;
  for (i = 2; i <= n / 2; ++i) {
     if (n % i == 0) {
      a = 1;
      break;
    }
  }
  if (a == 0){
    printf("Prime");
  }
  else{
    printf("Not Prime");
  } 
    return 0;
}

11.Manual Power Calculator:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b;
    scanf("%d %d",&a,&b);
    int c=pow(a,b);
    printf("%d",c);
    return 0;
}

12.Fibonacci Number 43:

int fib(int n){
    if (n == 0) {
        return 0;
    }
    else if (n == 1) {
        return 1;
    }
    return fib(n-1)+fib(n-2);
}

13.Tribonacci Number 3:

int tribonacci(int n) {
    int a=0,b=1,c=1;
    if(n==0){
        return 0;
    }
    else if(n==1||n==2){
        return 1;
    }
    int sum=0;
    for(int i=3;i<=n;i++){
        sum=a+b+c;
        a=b;
        b=c;
        c=sum;
    }
    return sum;
} 

14.NO OF COMMON FACTORS :

int commonFactors(int a,int b){
    int count =0;
    int limit =(a<b)?a:b;
    for(int i=1;i<=limit;i++){
        if(a%i==0 && b%i==0){
            count++;
        }
    }
    return count;
}

15.

bool isThree(int n) {
    if (n < 2) 
    return false;
    int count = 0;
    for (int i = 1; i * i <= n; ++i) {
    if (n % i == 0) {
    ++count;
    if (i != n / i) ++count;
    }
    }
    return count == 3;
    }

16.Count Digits:

int countDigits(int n) {
    int count=0;
    while(n!=0){
        n=n/10;
        count++;
    }
    return count; 
}

17.Maximum and Minimum Digit:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int n;
    scanf("%d",&n);
if(n==0){
  printf("0 0");
  return 0;
}

    if(n<0)
    {
        n=-n;
    }
    int count=0;
    int tem=n;
    int t=n;

    while(tem!=0)
    {
        tem=tem/10;
        count++;
    }
    int arr[count];
    for(int i=0;i<count;i++)
    {
        t=n%10;
        arr[i]=t;
        n=n/10;
    }
    int max=arr[0],min=arr[0];
    for(int i=1;i<count;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
            
        }
        if(arr[i]<min)
        {
            min=arr[i];
        }
    }
    printf("%d %d",max,min);
    return 0;
}

18.Reverse a Number:

#include <stdio.h>

int main() {
  int n;
  scanf("%d",&n);
  int rev=0;
  while(n>0){
     int r=n%10;
     rev=rev*10+r;
     n=n/10;
  }
  printf("%d",rev);
  return 0;
}

18.Maximum and Minimum Digit:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int n;
    scanf("%d",&n);
if(n==0){
  printf("0 0");
  return 0;
}

    if(n<0)
    {
        n=-n;
    }
    int count=0;
    int tem=n;
    int t=n;

    while(tem!=0)
    {
        tem=tem/10;
        count++;
    }
    int arr[count];
    for(int i=0;i<count;i++)
    {
        t=n%10;
        arr[i]=t;
        n=n/10;
    }
    int max=arr[0],min=arr[0];
    for(int i=1;i<count;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
            
        }
        if(arr[i]<min)
        {
            min=arr[i];
        }
    }
    printf("%d %d",max,min);
    return 0;
}

19.Palindrome Number :

bool isPalindrome(int x) {
    if(x<0){
        return false;
    }
     long long org=x;
    long long rev=0;
    while(x!=0){
       long long tem=x;
       x=x%10;
        rev=rev*10+x;
        x=tem/10;
    }
    if(org==rev){
        return true;
    }
        return false;
    }

20.Is it an Armstrong Number:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,n;
scanf("%d%n",&a,&n);
int count=0;
int temp=a;
int sum=0;
a=temp;
while(a>0){
int r=a%10;
int mul=1;
for(int i=1;i<=n;i++){
 mul=mul*r;
}
  sum=sum+mul;
  a=a/10;
}
if(sum==temp){
printf("true");
}
else
 printf("false");
return 0;
}




    
   



       
        

    
     
        
    

    















