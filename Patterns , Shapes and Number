1.Strong Number:

#include <stdio.h>
int main() {
int T;
scanf("%d", &T);
while (T--) {
    long long n, temp, sum = 0;
    scanf("%lld", &n);
    temp = n;
    while (temp > 0) {
        int digit = temp % 10;
        int fact = 1;
    for (int i = 1; i <= digit; i++) {
         fact *= i;
}
sum += fact;
temp /= 10;
}
if (sum == n)
    printf("YES\n");
else
    printf("NO\n");
}
    return 0;
}

2.Water Bottles:

int numWaterBottles(int numBottles, int numExchange) {
 int total_drunk=numBottles;
 int total_empty=numBottles;
 while(total_empty>=numExchange){
   int new_full=total_empty/numExchange;
   total_empty=total_empty%numExchange;
   total_drunk+=new_full;
   int empty=new_full+total_drunk;
 }
 return total_drunk;
}

3.Happy Number:

bool isHappy(int n) {
    int visited[1000] = {0};

 while (n != 1) {
if (visited[n] == 1)
 return false;   

visited[n] = 1;

int sum = 0;
 while (n > 0) {
int digit = n % 10;
  sum += digit * digit;
   n /= 10;
 }
 n = sum;
}
return true;

}

4.Rectangle Pattern:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b;
    scanf("%d %d",&a,&b);
    for(int i=1;i<=a;i++){
      for(int j=1;j<=b;j++){
         printf("*");
}
printf("\n");
 }
return 0;
}

5.Staircase of Stars:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    scanf("%d",&a);
    for(int i=1;i<=a;i++) {
     for(int j=1;j<=i;j++){
     printf("*");
     }
     printf("\n");
}
    return 0;
}

6.Descending Star Steps:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a;
    scanf("%d",&a);
    for(int i=a;i>=1;--i){
      for(int j=1;j<=i;++j)
{
         printf("*");
}
printf("\n");
}   
    return 0;
}

7.Binary Pattern :

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);
    int start = 1;   
    for (int i = N; i >= 1; i--) {
        int bit = start;
        for (int j = 1; j <= i; j++) {
            printf("%d", bit);
            bit = 1 - bit;
        }
        printf("\n");
        start = 1 - start;   
}
return 0;
}

8.Asterisk Pattern: X with Horizontal and Vertical Cut:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     int N;
    scanf("%d", &N);
    int mid = N / 2;
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {

            if (i == j || 
                i + j == N - 1 || 
                i == mid || 
                j == mid) {
                printf("* ");
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    return 0;  
}

9.Hollow Diamond Pattern:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n, i, j;
    scanf("%d", &n);
    for (i = 1; i <= n; i++) {
        for (j = i; j < n; j++) {
            printf(" ");
        }
        for (j = 1; j <= (2 * i - 1); j++) {
            if (j == 1 || j == (2 * i - 1))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }
    for (i = n - 1; i >= 1; i--) {
        for (j = n; j > i; j--) {
            printf(" ");
        }
        for (j = 1; j <= (2 * i - 1); j++) {
            if (j == 1 || j == (2 * i - 1))
                printf("*");
            else
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}

10.Filled Alphabet Pyramid Pattern:

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d", &N);
    for (int i = 1; i <= N; i++) {
        for (int s = 1; s <= (N - i) * 2; s++) {
            printf(" ");
        }
        char ch = 'A';
        for (int j = 1; j <= (2 * i - 1); j++) {
            printf("%c", ch);
            if (j < (2 * i - 1))
                printf(" ");
            ch++;
        }
        printf("\n");
    }
    return 0;
}
